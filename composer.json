{
    "name": "Gitilicious/gitilicious",
    "description": "Git platform",
    "keywords": ["project", "version control", "git"],
    "homepage": "https://github.com/Gitilicious/gitilicious",
    "license": "See the LICENSE file",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com",
            "homepage": "https://pieterhordijk.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {"Gitilicious\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Gitilicious\\Tests\\": "tests/src/"}
    },
    "require": {
        "php": ">=7",
        "rdlowrey/auryn": "1.1.*",
        "peehaa/minifine": "dev-master",
        "codecollab/http": "dev-master",
        "codecollab/authentication": "dev-master",
        "codecollab/form": "dev-master",
        "codecollab/i18n": "dev-master",
        "codecollab/router": "dev-master",
        "codecollab/template": "dev-master",
        "codecollab/theme": "dev-master",
        "swiftmailer/swiftmailer": "v5.4.*",
        "gitilicious/git-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    }
}
